{
  "name": "labadmin",
  "version": "0.1.0",
  "slug": "labadmin",
  "description": "A django app to manage your Fablab",
  "url": "https://www.labadmin.io",
  "startup": "application",
  "boot": "auto",
  "image": "officine/labadmin",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:8000]/labadmin/admin",
  "arch": [
    "armhf"
  ],
  "map": [
    "ssl"
  ],
  "options": {
    "db_host": "core-mariadb",
    "db_username": "labadmin",
    "db_password": "labadmin",
    "db_name": "labadmin",
    "secret_key": "https://www.miniwebtool.com/django-secret-key-generator/",
    "admin_name": "admin",
    "admin_email": "admin@example.com",
    "admin_pass": "password",
    "ssl": false
  },
  "schema": {
    "db_host": "str",
    "db_username": "str",
    "db_password": "str",
    "db_name": "str",
    "secret_key": "str",
    "admin_name": "str",
    "admin_email": "str",
    "admin_pass": "str",
    "ssl": "bool"
  },
  "ports": {
    "8000/tcp": 8000
  }
}
